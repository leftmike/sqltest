--
-- Test INSERT INTO expressions
--
DROP TABLE IF EXISTS tbl1;
CREATE TABLE tbl1 (c1 int, c2 int, c3 int);
INSERT INTO tbl1 VALUES
    (1 + 2, 3 * 4, 6 / 2),
    (12 * 6 + 3 * 2, 12 * 4 + 3, 12 + 4 * 3),
    (12 * (6 + 3), (12 + 4) * 3, 8 / 17);
SELECT * FROM tbl1;
    c1 c2 c3
    -- -- --
 1 108 48  0
 2   3 12  3
 3  78 51 24
(3 rows)
