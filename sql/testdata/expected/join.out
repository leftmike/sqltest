DROP TABLE IF EXISTS customers;
CREATE TABLE customers (customer_id int, customer varchar(128));
DROP TABLE IF EXISTS orders;
CREATE TABLE orders (order_id int, customer_id int, price int, item varchar(128));
DROP TABLE IF EXISTS empty_customers;
CREATE TABLE empty_customers (customer_id int, customer varchar(128));
DROP TABLE IF EXISTS empty_orders;
CREATE TABLE empty_orders (order_id int, customer_id int, price int, item varchar(128));
